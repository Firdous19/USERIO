{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*).html",
            "dest": "/$1.html"
        },
        {
            "src": "/(.*).(js|css)",
            "dest": "/$1.$2"
        },
        {
            "src": "/img/(.*)",
            "dest": "/img/$1"
        },
        {
            "src": "/(favicon.ico|manifest.json|robots.txt)",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}